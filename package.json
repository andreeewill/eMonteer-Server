{
  "name": "repair-backend",
  "version": "1.0.0",
  "description": "eMonteer (Emergency Vehicle Repairment App)",
  "main": "index.js",
  "scripts": {
    "start": "node lib/index.js",
    "start:dev": "nodemon --exec babel-node --extensions \".ts,.tsx\" src/index.ts --trace-warnings",
    "prebuild": "rimraf lib/",
    "tsc": "tsc",
    "build": "babel src --out-dir lib --extensions .ts,.tsx",
    "lint": "eslint --fix \"src/**/*.{ts,tsx,json}",
    "watch:babel": "babel --watch src --out-dir lib --extensions .ts,.tsx",
    "test": "jest --passWithNoTests --detectOpenHandles --forceExit",
    "cover": "jest --coverage --passWithNoTests --detectOpenHandles --forceExit",
    "watch": "jest --watch"
  },
  "author": "Andre William",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.19.1",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-winston": "^4.2.0",
    "winston": "^3.8.2"
  }
}
